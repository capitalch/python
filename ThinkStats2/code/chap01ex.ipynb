    "import pandas as pd\n",
    "import sqlanydb\n",
    "from string import Template\n",
    "\n",
    "mydf1 = pd.read_csv('mydata/test.txt')\n",
    "sql = '''select acc_code = func_getacccode(c.cp_acc_id)\n",
    "        , acc_name = func_getaccname(c.cp_acc_id)\n",
    "        , other = ''\n",
    "        , mdate = cp_date\n",
    "        , amount = cp_amt\n",
    "    from cash_payment c join acc_main  m on c.cp_acc_id = m.acc_id\n",
    "    where m.acc_type = 'E'\n",
    "\n",
    "UNION \n",
    "select acc_code = func_getacccode(c.acc_id)\n",
    "        , acc_name = func_getaccname(c.acc_id)\n",
    "        , other = ''\n",
    "        , mdate = cheq_date\n",
    "        , amount = cheq_amt\n",
    "    from cheque_payment c join acc_main  m on c.acc_id = m.acc_id\n",
    "    where m.acc_type = 'E'\n",
    "\n",
    "UNION \n",
    "select acc_code = func_getacccode(acc_id_exp)\n",
    "        , acc_name = func_getaccname(acc_id_exp)\n",
    "        , other = func_getaccname(acc_id)\n",
    "        , mdate = exp_date\n",
    "        , amount = exp_amt\n",
    "    from expence '''\n",
    "def getconn(db):\n",
    "    return sqlanydb.connect(uid='dba', pwd='sql', eng='server', dbn=db, host='kushserver' )\n",
    "\n",
    "def get_amount(row):\n",
    "    a=  (float(row['qty']) * float(row['price']))\n",
    "    return(a)\n",
    "    #return (\"{:,}\".format(a)) \n",
    "\n",
    "pd.options.display.float_format = '{:,.0f}'.format\n",
    "\n",
    "dfa = pd.read_sql_query(sql, getconn('capi2019'))\n",
    "dfa['month'] = pd.to_datetime(dfa['mdate']).dt.month\n",
    "dfa['amt'] = pd.to_numeric(dfa['amount'])\n",
    "p = dfa.pivot_table( index=['acc_code', 'acc_name', 'other'], columns='month', values='amt', aggfunc=sum, fill_value=0)\n",
    "p.to_excel('c:/expcapi2019.xlsx')\n",
    "#p.plot(kind='line', x='item', y='year')\n",
    "p.head(10000)"
   ]
